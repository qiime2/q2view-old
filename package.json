{
  "name": "qiime2-view",
  "version": "0.0.1",
  "description": "View QIIME 2 Visualizations on the web!",
  "main": "index.js",
  "scripts": {
    "lint:css": "stylelint 'app/**/*.css' --config config/stylelint.yaml",
    "lint:js": "node_modules/eslint/bin/eslint.js app/ config/ --config config/eslint.yaml",
    "lint": "npm run lint:js && npm run lint:css",
    "test": "npm run lint",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --host localhost --port 4242 --inline --hot",
    "build": "webpack --config config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiime2/view.qiime.org.git"
  },
  "author": "QIIME 2 Development Team",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/qiime2/view.qiime2.org/issues"
  },
  "homepage": "https://github.com/qiime2/view.qiime2.org#README.md",
  "dependencies": {
    "bootstrap-css-only": "^3.3.6",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "jszip": "^3.1.2",
    "lodash": "^4.16.4",
    "node-uuid": "^1.4.7",
    "serviceworker-webpack-plugin": "^0.1.5",
    "webpack": "^1.13.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "file-loader": "^0.9.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^13.0.2",
    "url-loader": "^0.5.7",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "private": true
}
