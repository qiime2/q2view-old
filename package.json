{
  "name": "qiime-view",
  "version": "0.0.1",
  "description": "View QIIME 2 Visualizations on the web!",
  "main": "index.js",
  "scripts": {
    "lint:css": "stylelint 'app/**/*.css' --config config/stylelint.yaml",
    "lint:js": "eslint app/ config/ --ext .js,.jsx --config config/eslint.yaml",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "npm run lint --silent",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "start": "webpack-dev-server --config config/webpack.dev.config.js --host localhost --port 4242 --inline --hot",
    "build": "webpack --config config/webpack.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiime2/qiime-view.git"
  },
  "author": "QIIME 2 Development Team",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/qiime2/qiime-view/issues"
  },
  "homepage": "https://github.com/qiime2/qiime-view#readme",
  "dependencies": {
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "chai": "^3.5.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.7.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "mocha": "^3.1.0",
    "style-loader": "^0.13.1",
    "stylelint": "^7.3.1",
    "stylelint-config-standard": "^13.0.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "private": true
}
